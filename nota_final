#include <stdio.h>

int main() {
    float a, b, c, notaFinal;

    printf("Digite sua nota do trabalho: ");
    scanf("%f", &a);

    printf("Digite sua nota do teste: ");
    scanf("%f", &b);

    printf("Digite sua nota da prova: ");
    scanf("%f", &c);

    notaFinal = a * 0.10 + b * 0.60 + c * 0.30;

    printf("Nota final: %f\n", notaFinal);

    return 0;
}
