#include <stdio.h>

int main() {
    int A, B;

    printf("Digite o valor de A: ");
    scanf("%d", &A);

    printf("Digite o valor de B: ");
    scanf("%d", &B);

    if (B == 0) {
        printf("Erro: divisao por zero nao eh permitida.\n");
    } else {
        printf("Resultado da divisao: %d\n", A / B);
    }

    return 0;
}
